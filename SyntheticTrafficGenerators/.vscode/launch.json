    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File with args",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": [
                "--frequency", "60",
                "--concurrent", "20",
                "--indelay", "100"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Local IIS tests",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": [
                "--frequency", "10",
                "--environment", "local"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: AppServices with Nuget",
            "type": "python",
